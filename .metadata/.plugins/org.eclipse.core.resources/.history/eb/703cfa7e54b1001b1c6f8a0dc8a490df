package vm.com;

public class fibonacci { 
static int fib(int n) { 
	if (n == 0) 
	return 0;
	if (n ==1 || n==2) {
		System.out.println(1);
		return 1;
	}
	System.out.println(fib(n-1) + fib(n-2));

return fib(n-1) + fib(n-2); 
} 

public static void main (String[] args) 
{ 
	int n = 5; 
	System.out.println(fib(n)); 
} 
}